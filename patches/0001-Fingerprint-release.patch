From: vgdn1942 <vgdn1942@gmail.com>
Date: Sat, 1 Aug 2020 14:42:22 +0300
Subject: [PATCH] Fingerprint & release

Change-Id: I8ac1293c6b144e449445bde5359fe51911f332e5
---
 core/Makefile             | 2 +-
 tools/buildinfo_common.sh | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/core/Makefile b/core/Makefile
index 1d0c63e23..e9a249d1c 100644
--- a/core/Makefile
+++ b/core/Makefile
@@ -304,7 +304,7 @@ endif
 ifeq ($(DEFAULT_SYSTEM_DEV_CERTIFICATE),build/make/target/product/security/testkey)
 BUILD_KEYS := test-keys
 else
-BUILD_KEYS := dev-keys
+BUILD_KEYS := release-keys
 endif
 BUILD_VERSION_TAGS += $(BUILD_KEYS)
 BUILD_VERSION_TAGS := $(subst $(space),$(comma),$(sort $(BUILD_VERSION_TAGS)))
diff --git a/tools/buildinfo_common.sh b/tools/buildinfo_common.sh
index 6041d79cd..da32c2016 100755
--- a/tools/buildinfo_common.sh
+++ b/tools/buildinfo_common.sh
@@ -12,7 +12,7 @@ echo "# autogenerated by $0"
 
 echo "ro.${partition}.build.date=`$DATE`"
 echo "ro.${partition}.build.date.utc=`$DATE +%s`"
-echo "ro.${partition}.build.fingerprint=$BUILD_FINGERPRINT"
+echo "ro.${partition}.build.fingerprint=Blackview/BV9500Plus_EEA/BV9500Plus:9/PPR1.180610.011/root.20190813.133031:user/release-keys"
 echo "ro.${partition}.build.id=$BUILD_ID"
 echo "ro.${partition}.build.tags=$BUILD_VERSION_TAGS"
 echo "ro.${partition}.build.type=$TARGET_BUILD_TYPE"
